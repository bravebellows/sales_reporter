<p id="notice"><%= notice %></p>

<h1>Csv Imports</h1>

<table>
  <thead>
    <tr>
      <th>Filename</th>
      <th>Imported</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @csv_imports.each do |csv_import| %>
      <tr>
        <td><%= csv_import.filename %></td>
        <td><%= csv_import.imported %></td>
        <td><%= link_to 'Show', csv_import %></td>
        <td><%= link_to 'Edit', edit_csv_import_path(csv_import) %></td>
        <td><%= link_to 'Destroy', csv_import, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csv Import', new_csv_import_path %>
